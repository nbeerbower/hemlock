import { random_bytes } from "@stdlib/crypto";

print("Testing random_bytes");
let buf = random_bytes(32);
print("Type of buf: " + typeof(buf));
print("Type of buf.length: " + typeof(buf.length));
print("Value of buf.length: " + typeof(buf.length));

if (buf.length == 32) {
    print("✓ Correct length");
} else {
    print("✗ Wrong length: " + typeof(buf.length));
}
