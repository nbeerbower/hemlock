// Test: Binary read/write
let buf = buffer(10);
let i = 0;
while (i < 10) {
    buf[i] = 65 + i;
    i = i + 1;
}
write_bytes("test_binary.dat", buf);
let data = read_bytes("test_binary.dat");
print("Length:");
print(data.length);
print("First byte:");
print(data[0]);
print("Last byte:");
print(data[9]);
free(buf);
free(data);
