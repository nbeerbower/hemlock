// Test recursive functions
fn factorial(n) {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

fn fibonacci(n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

fn sum_to(n) {
    if (n <= 0) {
        return 0;
    }
    return n + sum_to(n - 1);
}

print(factorial(5));
print(factorial(6));
print(fibonacci(10));
print(sum_to(10));
