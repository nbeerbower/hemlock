// Test environment variable builtins

// Set and get
setenv("TEST_VAR", "hello");
print(getenv("TEST_VAR"));

// Overwrite
setenv("TEST_VAR", "world");
print(getenv("TEST_VAR"));

// Non-existent (returns null)
let missing = getenv("NONEXISTENT_VAR_12345");
print(missing == null);

// Process ID (just check it returns a number > 0)
let pid = get_pid();
print(pid > 0);

print("done");
