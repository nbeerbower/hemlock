// Test typed arrays

// Basic typed array
let nums: array<i32> = [1, 2, 3, 4, 5];
print(nums[0]);
print(nums.length);

nums.push(6);
print(nums[5]);

// String typed array
let names: array<string> = ["Alice", "Bob"];
print(names[0]);
names.push("Charlie");
print(names[2]);

// Explicit untyped array
let mixed: array = [1, "hello", true];
print(mixed[0]);
print(mixed[1]);
print(mixed[2]);

print("done");
