// Test higher-order functions: map, filter, reduce

// map - transform elements
let nums = [1, 2, 3, 4, 5];
let doubled = nums.map(fn(x) {
    return x * 2;
});
for (let x in doubled) {
    print(x);
}

// filter - select elements
let evens = nums.filter(fn(x) {
    return x % 2 == 0;
});
for (let x in evens) {
    print(x);
}

// reduce - accumulate
let sum = nums.reduce(fn(acc, x) {
    return acc + x;
}, 0);
print(sum);

print("done");
