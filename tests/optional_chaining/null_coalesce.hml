// Test null coalescing operator

// Basic null coalescing
let a = null;
let b = 10;
print(a ?? b);  // 10

// Non-null value
let c = 20;
let d = 30;
print(c ?? d);  // 20

// Multiple null coalescing
let e = null;
let f = null;
let g = 40;
print(e ?? f ?? g);  // 40

// Null coalescing with expressions
let obj = null;
let fallback = { name: "Default" };
let result = obj ?? fallback;
print(result.name);  // Default

// Null coalescing with zero (should NOT treat zero as null)
let zero = 0;
print(zero ?? 100);  // 0

// Null coalescing with false (should NOT treat false as null)
let bool_val = false;
print(bool_val ?? true);  // false

// Null coalescing with empty string (should NOT treat "" as null)
let empty = "";
print(empty ?? "default");  // "" (empty string)
