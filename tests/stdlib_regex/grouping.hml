// Test: Grouping and alternation
import { test } from "@stdlib/regex";

// Alternation
print(test("^(cat|dog)$", "cat", null));          // true
print(test("^(cat|dog)$", "dog", null));          // true
print(test("^(cat|dog)$", "bird", null));         // false

// Grouping with quantifiers
print(test("^(ab)+$", "ab", null));               // true
print(test("^(ab)+$", "abab", null));             // true
print(test("^(ab)+$", "aba", null));              // false

// Complex alternation
print(test("^(http|https)://", "http://example.com", null));   // true
print(test("^(http|https)://", "https://example.com", null));  // true
print(test("^(http|https)://", "ftp://example.com", null));    // false

// Nested groups
print(test("^((ab|cd)+)$", "abcdab", null));      // true
print(test("^((ab|cd)+)$", "abef", null));        // false
